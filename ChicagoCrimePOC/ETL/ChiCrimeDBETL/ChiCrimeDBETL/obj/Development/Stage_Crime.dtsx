	,CAST(beat as Varchar(10)) as PoliceBeatCode
	,CAST(district as Varchar(10)) as PoliceDistrictCode
	,CAST(ward as Varchar(10)) as WardCode
	,CAST(community_area as Varchar(10)) as CommunityAreaCode
	,CAST(fbi_code as Varchar(10)) as FBICode
	,CAST(x_coordinate as Varchar(20)) as X_Coordinate
	,CAST(y_coordinate as Varchar(20)) as Y_Coordiante
	,CAST(year as char(4)) as CrimeYear
	,CAST(updated_on as datetime) as SourceLastUpdateDate
	,CASE WHEN RTRIM(LTRIM(latitude)) = '' THEN NULL
		ELSE CAST(latitude as DECIMAL(9,6))
		END Latitude
	,CASE WHEN RTRIM(LTRIM(longitude)) = '' THEN NULL
		ELSE CAST(longitude as DECIMAL(9,6))
		END Longitude
FROM LAND.Crimes</property>
