                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=13.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT CAST(ID as Varchar(255)) as CrimeID
	,CAST(Case_Number as Varchar(20)) as CaseNo
	,CAST([date] as datetime) as CrimeDate
	,CAST(block as Varchar(255)) as StreetBlockLocation
